DECLARE @c INT;
SET @c = (SELECT COUNT(*) FROM STATION);

SELECT 
    CAST(ROUND(AVG(LAT_N), 4) AS DECIMAL(10, 4)) AS median
FROM (
    SELECT LAT_N 
    FROM STATION 
    ORDER BY LAT_N 
    OFFSET (@c - 1) / 2 ROWS 
    FETCH NEXT CASE WHEN @c % 2 = 0 THEN 2 ELSE 1 END ROWS ONLY
) AS MedianValues;
