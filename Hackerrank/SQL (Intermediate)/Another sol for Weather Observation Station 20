SELECT CAST(ROUND(
    (
        (SELECT MAX(LAT_N) 
         FROM (SELECT TOP 50 PERCENT LAT_N FROM STATION ORDER BY LAT_N) AS maxx)
        +
        (SELECT MIN(LAT_N) 
         FROM (SELECT TOP 50 PERCENT LAT_N FROM STATION ORDER BY LAT_N DESC) AS minn)
    ) / 2, 4) AS DECIMAL(10, 4)) AS median;
